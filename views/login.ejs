

<!DOCTYPE html>
<html>

<head>
  <title></title>
  <!-- JavaScript Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>

  <link rel='stylesheet' href='css1/login.css' />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>

<body>

  <section class="h-100 gradient-form" style="background-color: #eee;">
    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center  h-100">

        
          
              <div class="col-lg-1"></div>
              <div class="col-lg-5">
                <img src="https://fedena.com/blog/wp-content/uploads/2020/05/student-enrollment-with-online-admission-system.png" class="img1">
                <!-- .<img src="https://dxavhl1p2dibq.cloudfront.net/sites/default/files/2022-03/Admission%20Management%20system.jpg" class="img1"> -->
              </div>
              <div class="col-lg-1"></div>
            <div class="col-lg-5">
              <div class="card-body p-md-5 mx-md-4">

                <div class="text-center">
                  <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-login-form/lotus.webp"
                    style="width: 185px;" alt="logo">
                  <h4 class="mt-1 mb-5 pb-1">ADMISSION PORTAL</h4>
                </div>

                <form action="/user/login12" method="post">
                  <%if(message.length> 0){ %>
                    <div class="alert alert-danger">
                      <%=message %>
                    </div>
                    <% }%>
                      <p>Please login to your account</p>

                      <div class="form-outline mb-4">
                        <input type="email" class="form-control" placeholder="enter your email address" name="email">
                        <label class="form-label">Username</label>
                      </div>

                      <div class="form-outline mb-4">
                        <input type="password" class="form-control" placeholder="enter your password" name="password">
                        <label class="form-label" f>Password</label>
                      </div>

                      <div class="text-center pt-1 mb-5 pb-1">
                        <button type="submit" class="btn btn-success btn-lg mb-1">Log In</button>

                      </div>

                      <div class="d-flex align-items-center justify-content-center pb-4">
                        <p class="mb-0 me-2">Don't have an account?</p>
                        <button type="button" class="btn btn-primary" ><a href="/signup" class="c" style="text-decoration: none; color: white;">Create
                            New</a></button>
                      </div>

                </form>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


  </section>

</body>

</html>

<%- include('./footer.ejs')%>